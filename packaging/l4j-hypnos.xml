<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <chdir>.</chdir>
  <priority>normal</priority>
  <stayAlive>false</stayAlive>
  <icon>icon.ico</icon>
  <jre>
    <path>jre</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64/32</runtimeBits>
    <opt>-Djava.library.path=&quot;lib&quot;</opt>
    <opt>-Xmx600m</opt>
    <opt>-XX:+UseG1GC</opt>
    <opt>-XX:+UseStringDeduplication</opt>
    <opt>-XX:MinHeapFreeRatio=20</opt>
	<opt>-XX:MaxHeapFreeRatio=30</opt>
	<opt>--module-path ./jre/jfx/lib</opt>
	<opt>--add-modules=javafx.controls,javafx.swing</opt>
	<opt>--add-opens javafx.controls/javafx.scene.control=ALL-UNNAMED</opt>
	<opt>-Djdk.gtk.version=2</opt>
  </jre>
</launch4jConfig>



